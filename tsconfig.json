{
	"compilerOptions": {
		"target": "ES2022",
		"module": "ES2022",
		"moduleResolution": "bundler",
		"strict": true,
		"lib": ["ES2022", "WebWorker"],
		"types": ["@cloudflare/workers-types", "node", "vitest"],
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"skipLibCheck": true
	},
	"include": ["src/**/*.ts", "tests/**/*.ts", "examples/**/*.ts"]
}
